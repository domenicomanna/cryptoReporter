{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "lint": "eslint --fix './**/*.{js,jsx,ts,tsx}'",
    "generateSdk": "yarn downloadSwaggerSpec && rimraf src/api/generatedSdk/* && openapi-generator-cli generate -i src/api/swagger.json -g  typescript-fetch -o src/api/generatedSdk --type-mappings=Date=string",
    "downloadSwaggerSpec": "run-script-os",
    "downloadSwaggerSpec:nix": "curl http://localhost:5000/swagger/v1/swagger.json --output ./src/api/swagger.json",
    "downloadSwaggerSpec:windows": "curl http://localhost:5000/swagger/v1/swagger.json --Outfile ./src/api/swagger.json"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.7",
    "@mui/lab": "^7.0.1-beta.21",
    "@mui/material": "^7.3.7",
    "@mui/x-date-pickers": "^8.24.0",
    "@tanstack/react-query": "^5.90.16",
    "formik": "^2.4.9",
    "luxon": "^3.7.2",
    "material-react-table": "^3.2.1",
    "mui-file-input": "^7.0.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "react-toastify": "^11.0.5",
    "web-vitals": "^5.1.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.6",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "run-script-os": "^1.1.6",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.3.1 ",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
